algoritmo "PASSO 01 - 11 - SEQUÊNCIAS BÁSICAS"
var
   a, b, c: inteiro
   delta: real
inicio
   Escreval ("Informe um valor para A:")
   Leia (a)
   Escreval ("Informe um valor para B:")
   Leia (b)
   Escreval ("Informe um valor para C:")
   Leia (c)
   delta <- ((b^2)-(4*a*c))
   Escreval ("O valor de delta é ", delta, ".")
fimalgoritmo